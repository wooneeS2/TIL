return <p>Loading....</p>;\n    }\n\n    if (error) {\n        return <p>Faild to load</p>;\n    }\n\n    if (!sales) {\n        return <p>Loading...</p>;\n    }\n\n    return (\n        <ul>\n            {sales.map(sale => {\n                return (\n                    <li key={sale.id}>\n                        {sale.userName} - ${sale.value}\n                    </li>\n                );\n            })}\n        </ul>\n    );\n}\n\nexport default LastSalesPage;\n"]},"metadata":{},"sourceType":"module"}